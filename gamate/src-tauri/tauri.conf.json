{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Gamate",
  "version": "0.1.0",
  "identifier": "com.administrator.gamate",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Gamate",
        "width": 1280,
        "height": 960
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/logo.svg", "icons/icon.ico"]
  }
}
